{{>header}}

import * as Types from '../data-types/types{{>tsExtention}}';
import { createSwrHook } from '{{{factories}}}';

{{#each service.operations}}
export const use{{{capitalize name}}} = createSwrHook<Types.{{{@root.service.name}}}{{{capitalize name}}}Input, Types.{{{@root.service.name}}}{{{capitalize name}}}Output>({
	route: '{{{path}}}',
	method: '{{{method}}}'{{#if parametersBody}}{{#if parametersBody.mediaType}},
	mediaType: '{{{parametersBody.mediaType}}}'{{/if}}{{/if}},
});
{{/each}}
